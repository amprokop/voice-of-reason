{
  "manifest_version": 2,
  "name": "Nuclear Option",
  "description": "Self-control tool. Requires laborious typing to visit non-approved domains.",
  "version": "1.0",
  "content_scripts": [{
      "matches":["<all_urls>"],
      "js":["index.js"],
      "run_at": "document_start"
  }],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
